{
  "name": "rhysnhall/etsy-php-sdk",
  "version": "1.0.1",
  "description": "PHP SDK for Etsy API v3.",
  "type": "library",
  "keywords": [
    "etsy",
    "php",
    "api",
    "sdk"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Rhys Hall",
      "email": "hello@rhyshall.com",
      "homepage": "https://github.com/rhysnhall"
    }
  ],
  "require": {
    "php": "^7.4",
    "guzzlehttp/guzzle": "^6.5"
  },
  "autoload": {
